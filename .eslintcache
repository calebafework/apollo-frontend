v>\n                <input \n                    type=\"text\"\n                    name=\"comment\"\n                    value={ comment.content}\n                    onChange= {handleChange}\n                />\n            </div>\n        </div>\n    )\n\n    return (\n        <div>\n            <iframe src={\"https://open.spotify.com/embed/playlist/\"+ url} width=\"300\" height=\"380\" frameBorder=\"0\" allowtransparency=\"true\" allow=\"encrypted-media\"></iframe>\n            { editMode ? editInfo : displayInfo }\n                <div>           \n                    <button onClick={ handleDelete } className=\"deleteButton\">Delete</button>\n                    <button onClick={ editComment } className=\"editButton\">{ editMode ? \"Save Changes\" : \"Edit\" }</button>\n                </div>    \n        </div> \n    );\n}\n\nexport default CommentShow;","/home/caleb/Documents/GA/unit4/project4/Apollo/apollo-frontend/src/pages/Home.js",["91"],"import React from 'react';\nimport CommentShow from '../components/CommentShow';\nimport CreateComment from '../components/CreateComment';\nimport './Home.css'\n\nconst Home = (props) => {\n\n    return (\n        <div>\n            <h3 id=\"description\">Named after the Olympian God of music, Apollo takes an input and curates a playlist based off the mood of your input. To use Apollo simply write a brief paragraph about how you're feeling. Apollo will then analyze the tone of your paragraph and provide you with a Spotify playlist. From there, you will sign into your Spotify account to listen to the playlist. </h3>\n            <h3>To use Apollo simply tell us, in a few sentences, how you're doing today.</h3>         \n            <CreateComment { ...props } />\n        </div>\n    );\n}\n\nexport default Home;\n","/home/caleb/Documents/GA/unit4/project4/Apollo/apollo-frontend/src/pages/Comments.js",["92","93","94"],"import CommentList from \"../components/CommentList\"\nimport React, {useState, useEffect} from 'react';